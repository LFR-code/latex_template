	doi = {http://dx.doi.org/10.1016/S0095-0696(03)00024-X},
	issn = {0095-0696},
	journal = {Journal of Environmental Economics and Management},
	number = {2},
	pages = {183 - 206},
	title = {Economic {\em impacts} of marine reserves: the importance of spatial behavior},
